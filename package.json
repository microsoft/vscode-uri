{
  "name": "vscode-uri",
  "author": "Microsoft",
  "version": "3.0.7",
  "description": "The URI implementation that is used by VS Code and its extensions",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "default": "./lib/cjs/index.js"
    }
  },
  "imports": {
    "#path": {
      "import": {
        "browser": "./lib/esm/path-browserify.js",
        "default": "./lib/esm/path.js"
      },
      "default": {
        "browser": "./lib/cjs/path-browserify.js",
        "default": "./lib/cjs/path.js"
      }
    }
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib",
    "prepack": "npm run clean && tsc -b && node fix-esm.mjs",
    "test": "npm run prepack && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/vscode-uri.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-uri/issues"
  },
  "homepage": "https://github.com/microsoft/vscode-uri#readme",
  "dependencies": {
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/path-browserify": "^1.0.0",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.0"
  }
}
